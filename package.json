  "keywords": [
    "Google",
    "Maps",
    "Directions",
    "Distance Matrix",
    "Elevation",
    "Geocoding",
    "Geolocation",
    "Places",
    "Roads",
    "Street View",
    "Time Zone",
    "Zod",
    "TypeScript"
  ],
  "author": "Vitaly Budovski <vbudovski@gmail.com>",
  "license": "MIT",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "provenance": true
  },
  "files": ["dist/*", "CHANGELOG.md", "LICENSE.md"],
  "repository": "https://github.com/vbudovski/google-maps.git",
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@changesets/cli": "^2.27.2",
    "@kubb/cli": "^2.18.8",
    "@kubb/core": "^2.18.8",
    "@kubb/plugin-oas": "^2.18.8",
    "@kubb/react": "^2.18.8",
    "@kubb/swagger": "^2.18.8",
    "@kubb/swagger-client": "^2.18.8",
    "@kubb/swagger-zod": "^2.18.8",
    "@parcel/packager-ts": "2.12.0",
    "@parcel/transformer-typescript-types": "2.12.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@vitest/coverage-v8": "^1.6.0",
    "dotenv": "^16.4.5",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "parcel": "^2.12.0",
    "react": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "zod-fetch": "^0.1.1"
  },
  "peerDependencies": {
    "zod": "^3.22.2"
  },
  "scripts": {
    "generate": "kubb && biome check --apply-unsafe src/generated",
